    .thread T_0, $push;
    .scope S_0x64a187a28700;
T_1 ;
    %vpi_call 2 19 "$dumpfile", "testbench_barrel_shifter_only.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x64a187a28700 {0 0 0};
