    %store/vec4 v0x64a187a3db10_0, 0, 10;
    %jmp T_0.9;
T_0.0 ;
    %load/vec4 v0x64a187a3da10_0;
    %store/vec4 v0x64a187a3db10_0, 0, 10;
    %jmp T_0.9;
T_0.1 ;
    %load/vec4 v0x64a187a3da10_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v0x64a187a3db10_0, 0, 10;
    %jmp T_0.9;
T_0.2 ;
    %load/vec4 v0x64a187a3da10_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
